        styles: [Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"])(__webpack_require__(/*! ./clientes.page.scss */ "./src/app/clientes/clientes.page.scss")).default]
    })
], ClientesPage);



