let ClientesUpdatePage = class ClientesUpdatePage {
    constructor(formBuilder, clientServ, template, route, firestore) {
        this.formBuilder = formBuilder;
        this.clientServ = clientServ;
        this.template = template;
        this.route = route;
        this.firestore = firestore;
        this.cliente = new _model_cliente__WEBPACK_IMPORTED_MODULE_2__["Cliente"]();
        this.iniciarForm();
    }
    ngOnInit() {
        this.route.paramMap.subscribe(url => {
            let id = url.get('id');
            this.clientServ.buscaPorId(id).subscribe(data => {
                this.cliente = data.payload.data();
                this.cliente.id = data.payload.id;
                this.iniciarForm();
            });
        });
    }
    iniciarForm() {
        this.formGroup = this.formBuilder.group({
            nome: [this.cliente.nome, [_angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].minLength(5)]],
            cpf: [this.cliente.cpf, [_angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].minLength(5)]],
            telefone: [this.cliente.telefone, [_angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].minLength(5)]],
            email: [this.cliente.email, [_angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].minLength(5)]],
            endereco: [this.cliente.endereco, [_angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].minLength(5)]]
        });
    }
    atualizar() {
        this.clientServ.atualizar(this.cliente.id, this.formGroup.value).subscribe(data => {
            console.log(data);
            this.template.myAlert('Atualizado com sucesso');
        });
    }
};
