let CadastrarClientePage = class CadastrarClientePage {
    constructor(formBuilder, auth, navCtrl, menuCtrl, template, afs) {
        this.formBuilder = formBuilder;
        this.auth = auth;
        this.navCtrl = navCtrl;
        this.menuCtrl = menuCtrl;
        this.template = template;
        this.afs = afs;
        this.teste = {};
        this.message = null;
        this.iniciarForm();
    }
    ngOnInit() {
    }
    iniciarForm() {
        this.formGroup = this.formBuilder.group({
            username: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].email]],
            password: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].minLength(13), _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].maxLength(16)]],
            nome: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].minLength(13), _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].maxLength(32)]],
            grupo: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].minLength(1), _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].maxLength(16)]],
            deficiencia: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].minLength(13), _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].maxLength(16)]],
            cpf: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].minLength(11), _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].maxLength(11)]],
            cep: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].minLength(8), _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].maxLength(8)]],
            endereco: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].minLength(1), _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].maxLength(40)]],
        });
    }
    // Para cadastrar login junto com informações na coleção Cadastros_de_clientes
    cadastrarUsers() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            let user = this.formGroup.controls['username'].value;
            let pass = this.formGroup.controls['password'].value;
            try {
                const newUser = yield this.auth.createUserWithEmailAndPassword(user, pass);
                /* Serve para criar uma senha e login, pegar o id do fire base o "uid" e salvar todos os valores do formGroup
                na coleção "cadastro de clientes"*/
                yield this.afs.collection('Cadastros_de_clientes').doc(newUser.user.uid).set(this.formGroup.value).then(() => {
                    this.message = "Informações cadastradas com sucesso!";
                    this.formGroup.reset();
                }).catch(() => {
                    this.message = "Erro ao cadastrar as Informações!";
                });
                console.log('Cadastro efetuado com sucesso!');
            }
            catch (error) {
                if (error === "auth/email-already-in-use") {
                    console.log(`email já esta em uso ${error}`);
                }
                else {
                    console.log(`O erro é ${error}`);
                }
            }
        });
    }
    autenticar() {
        let user = this.formGroup.controls['username'].value;
        let pass = this.formGroup.controls['password'].value;
        this.template.loading.then(load => {
            load.present();
            this.auth.signInWithEmailAndPassword(user, pass).then(data => {
                load.dismiss();
                this.menuCtrl.enable(true);
                this.navCtrl.navigateRoot(['itensvencidos']);
            }).catch(data => {
                load.dismiss();
                this.template.myAlert("Usuário ou senha inválidos");
            });
        });
    }
};
